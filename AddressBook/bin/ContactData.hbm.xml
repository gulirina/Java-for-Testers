<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
 <class name="com.example.tests.ContactData" table="addressbook">
 <id name="id" column="id" ></id>
 <property name="firstName" column="firstname" />
 <property name="secondName" column="lastname" />
 <property name="mainAddress" column="address" />
 <property name="hPhone" column="home" />
 <property name="mPhone" column="mobile" />
 <property name="wPhone" column="work" />
 <property name="email1" column="email" />
 <property name="email2" column="email2" />
 <property name="bDay" column="bday" />
 <property name="bMonth" column="bmonth" />
 <property name="bYear" column="byear" />
 <property name="supAddress" column="address2" />
 <property name="supPhone" column="phone2" />	
 </class>
</hibernate-mapping>
